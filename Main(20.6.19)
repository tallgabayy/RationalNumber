package com.company;

public class Main {

    public static void main(String[] args) {
	RationalNumber r = new RationalNumber (1, 3);
	System.out.println(r);
    }
    public static Number max (Number[] numbers) {
        if (numbers.length == 0)
            return null;
        Number maxNumber = numbers[0];
        double max = maxNumber.doubleValue();
        for (Number number : numbers) {
            if (number.doubleValue() > max) {
                maxNumber = number;
                max = number.doubleValue();
            }
        }
        return maxNumber;
    }


    public static double sum (Number [] numbers) {
        double sum = 0;
        for (Number number : numbers) {
            sum += number.doubleValue();
        }
        return sum;
    }

    public int add (){



    }

}
